{
	"name": "snd-backoffice",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest --config vitest.config.unit.ts",
		"test:integration": "vitest --config vitest.config.integration.ts",
		"test:browser": "playwright test",
		"build:create-db-scripts-dev": "vite-node --mode development src-database/index.ts create-sql-scripts",
		"build:create-db-scripts-prd": "vite-node --mode production src-database/index.ts create-sql-scripts",
		"build:create-db-docker-image": "vite-node src-database/index.ts create-docker-image",
		"build:docker-image": "npm run build:create-db-scripts-dev && npm run build:create-db-docker-image",
		"docker:db-start": "docker run --rm --name snd-backoffice-db-dev --env-file ./.env.development.local -p 5432:5432 -d snd-backoffice-db-dev",
		"docker:db-stop": "docker stop snd-backoffice-db-dev"
	},
	"devDependencies": {
		"@playwright/test": "^1.40.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.3",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@testing-library/jest-dom": "^6.1.5",
		"@testing-library/svelte": "^4.0.5",
		"@types/node": "^20.10.5",
		"@vercel/postgres": "^0.5.1",
		"@vitest/ui": "^1.0.4",
		"jsdom": "^23.0.1",
		"prettier": "3.1.0",
		"prettier-plugin-sql": "^0.18.0",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"sveltekit-superforms": "^1.12.0",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.10",
		"vite-node": "^1.0.4",
		"vitest": "^1.0.4",
		"vitest-dom": "^0.1.1",
		"zod": "^3.22.4"
	},
	"type": "module"
}